{
  "version": 2,
  "builds": [
    {
      "src": "web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/web/images/$1"
    },
    {
      "src": "/logo.svg",
      "dest": "/web/logo.svg"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ]
}
