<svg width="800" height="500" viewBox="0 0 800 500" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#667eea;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#764ba2;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="card" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:0.9" />
      <stop offset="100%" style="stop-color:#f8f9ff;stop-opacity:0.9" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="500" fill="url(#bg)"/>
  
  <!-- Floating 3D elements -->
  <g transform="translate(100, 80)">
    <rect x="0" y="0" width="30" height="30" fill="#ffffff" opacity="0.3" transform="rotate(45 15 15)"/>
  </g>
  <g transform="translate(650, 120)">
    <circle cx="0" cy="0" r="20" fill="#ffffff" opacity="0.2"/>
  </g>
  <g transform="translate(700, 400)">
    <polygon points="0,0 20,10 0,20" fill="#ffffff" opacity="0.25"/>
  </g>
  
  <!-- Main card -->
  <rect x="100" y="150" width="600" height="200" rx="20" fill="url(#card)" stroke="#ffffff" stroke-width="2"/>
  
  <!-- 3D Portfolio elements -->
  <g transform="translate(200, 200)">
    <!-- 3D Cube -->
    <g transform="translate(0, 0)">
      <!-- Front face -->
      <rect x="0" y="0" width="40" height="40" fill="#667eea" opacity="0.8"/>
      <!-- Top face -->
      <polygon points="0,0 20,-20 60,-20 40,0" fill="#5a6fd8" opacity="0.8"/>
      <!-- Right face -->
      <polygon points="40,0 60,-20 60,20 40,40" fill="#4c63d2" opacity="0.8"/>
    </g>
    
    <!-- 3D Sphere -->
    <g transform="translate(80, 10)">
      <circle cx="20" cy="20" r="20" fill="#764ba2" opacity="0.8"/>
      <ellipse cx="20" cy="15" rx="15" ry="8" fill="#ffffff" opacity="0.3"/>
    </g>
  </g>
  
  <!-- Interactive elements -->
  <g transform="translate(400, 180)">
    <rect x="0" y="0" width="200" height="120" rx="10" fill="#f8f9fa" stroke="#dee2e6" stroke-width="1"/>
    
    <!-- Navigation elements -->
    <rect x="20" y="20" width="60" height="15" rx="3" fill="#667eea" opacity="0.8"/>
    <text x="50" y="31" text-anchor="middle" font-family="Arial, sans-serif" font-size="8" fill="#ffffff">Home</text>
    
    <rect x="90" y="20" width="60" height="15" rx="3" fill="#764ba2" opacity="0.8"/>
    <text x="120" y="31" text-anchor="middle" font-family="Arial, sans-serif" font-size="8" fill="#ffffff">Projects</text>
    
    <rect x="160" y="20" width="60" height="15" rx="3" fill="#667eea" opacity="0.8"/>
    <text x="190" y="31" text-anchor="middle" font-family="Arial, sans-serif" font-size="8" fill="#ffffff">Contact</text>
    
    <!-- 3D content area -->
    <rect x="20" y="50" width="160" height="50" rx="5" fill="#ffffff" stroke="#e9ecef" stroke-width="1"/>
    <rect x="25" y="55" width="150" height="40" fill="#f8f9fa"/>
    
    <!-- 3D effect lines -->
    <line x1="25" y1="55" x2="30" y2="50" stroke="#dee2e6" stroke-width="1"/>
    <line x1="175" y1="55" x2="180" y2="50" stroke="#dee2e6" stroke-width="1"/>
    <line x1="25" y1="95" x2="30" y2="90" stroke="#dee2e6" stroke-width="1"/>
    <line x1="175" y1="95" x2="180" y2="90" stroke="#dee2e6" stroke-width="1"/>
  </g>
  
  <!-- Text -->
  <text x="400" y="280" text-anchor="middle" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#2c3e50">3D Interactive Portfolio</text>
  <text x="400" y="310" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" fill="#7f8c8d">Immersive user experience</text>
  
  <!-- Decorative elements -->
  <rect x="120" y="170" width="4" height="30" fill="#667eea" opacity="0.6"/>
  <rect x="120" y="170" width="30" height="4" fill="#667eea" opacity="0.6"/>
  <rect x="650" y="170" width="4" height="30" fill="#764ba2" opacity="0.6"/>
  <rect x="636" y="170" width="30" height="4" fill="#764ba2" opacity="0.6"/>
</svg>
