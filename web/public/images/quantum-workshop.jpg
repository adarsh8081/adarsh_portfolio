<svg width="800" height="500" viewBox="0 0 800 500" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#8e44ad;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#9b59b6;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="card" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:0.9" />
      <stop offset="100%" style="stop-color:#f8f5ff;stop-opacity:0.9" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="500" fill="url(#bg)"/>
  
  <!-- Floating quantum elements -->
  <g transform="translate(100, 80)">
    <circle cx="0" cy="0" r="25" fill="#ffffff" opacity="0.2"/>
    <circle cx="0" cy="0" r="15" fill="#8e44ad" opacity="0.3"/>
    <circle cx="0" cy="0" r="5" fill="#ffffff" opacity="0.5"/>
  </g>
  <g transform="translate(650, 120)">
    <rect x="0" y="0" width="30" height="30" fill="#ffffff" opacity="0.2" transform="rotate(45 15 15)"/>
  </g>
  <g transform="translate(700, 400)">
    <polygon points="0,0 25,12.5 0,25" fill="#ffffff" opacity="0.25"/>
  </g>
  
  <!-- Main card -->
  <rect x="100" y="150" width="600" height="200" rx="20" fill="url(#card)" stroke="#ffffff" stroke-width="2"/>
  
  <!-- Quantum computing elements -->
  <g transform="translate(200, 200)">
    <!-- Quantum circuit -->
    <g transform="translate(0, 0)">
      <!-- Qubit lines -->
      <line x1="0" y1="20" x2="80" y2="20" stroke="#8e44ad" stroke-width="2"/>
      <line x1="0" y1="40" x2="80" y2="40" stroke="#8e44ad" stroke-width="2"/>
      <line x1="0" y1="60" x2="80" y2="60" stroke="#8e44ad" stroke-width="2"/>
      
      <!-- Quantum gates -->
      <rect x="15" y="10" width="20" height="20" fill="#9b59b6" opacity="0.8"/>
      <text x="25" y="25" text-anchor="middle" font-family="Arial, sans-serif" font-size="8" fill="#ffffff">H</text>
      
      <rect x="35" y="30" width="20" height="20" fill="#8e44ad" opacity="0.8"/>
      <text x="45" y="45" text-anchor="middle" font-family="Arial, sans-serif" font-size="8" fill="#ffffff">X</text>
      
      <rect x="55" y="50" width="20" height="20" fill="#9b59b6" opacity="0.8"/>
      <text x="65" y="65" text-anchor="middle" font-family="Arial, sans-serif" font-size="8" fill="#ffffff">Z</text>
    </g>
  </g>
  
  <!-- Workshop content -->
  <g transform="translate(400, 180)">
    <rect x="0" y="0" width="200" height="120" rx="10" fill="#f8f9fa" stroke="#dee2e6" stroke-width="1"/>
    
    <!-- Workshop topics -->
    <text x="10" y="20" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#8e44ad">Workshop Topics:</text>
    <text x="10" y="35" font-family="Arial, sans-serif" font-size="10" fill="#666666">• Quantum Algorithms</text>
    <text x="10" y="50" font-family="Arial, sans-serif" font-size="10" fill="#666666">• Quantum Gates</text>
    <text x="10" y="65" font-family="Arial, sans-serif" font-size="10" fill="#666666">• Qiskit Implementation</text>
    <text x="10" y="80" font-family="Arial, sans-serif" font-size="10" fill="#666666">• Linear Algebra</text>
    
    <text x="100" y="100" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#8e44ad">50+ Students Trained</text>
  </g>
  
  <!-- Text -->
  <text x="400" y="280" text-anchor="middle" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#2c3e50">Quantum Computing Workshop</text>
  <text x="400" y="310" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" fill="#7f8c8d">Advanced quantum education</text>
  
  <!-- Decorative elements -->
  <rect x="120" y="170" width="4" height="30" fill="#8e44ad" opacity="0.6"/>
  <rect x="120" y="170" width="30" height="4" fill="#8e44ad" opacity="0.6"/>
  <rect x="650" y="170" width="4" height="30" fill="#9b59b6" opacity="0.6"/>
  <rect x="636" y="170" width="30" height="4" fill="#9b59b6" opacity="0.6"/>
</svg>
