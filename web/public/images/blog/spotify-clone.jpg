<svg width="800" height="500" viewBox="0 0 800 500" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1db954;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1ed760;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="card" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#191414;stop-opacity:0.95" />
      <stop offset="100%" style="stop-color:#121212;stop-opacity:0.95" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="500" fill="url(#bg)"/>
  
  <!-- Floating music notes -->
  <g transform="translate(100, 80)">
    <circle cx="0" cy="0" r="3" fill="#ffffff" opacity="0.8"/>
    <path d="M0,0 Q10,-10 20,0" stroke="#ffffff" stroke-width="2" fill="none" opacity="0.8"/>
  </g>
  <g transform="translate(650, 120)">
    <circle cx="0" cy="0" r="3" fill="#ffffff" opacity="0.6"/>
    <path d="M0,0 Q15,-15 30,0" stroke="#ffffff" stroke-width="2" fill="none" opacity="0.6"/>
  </g>
  
  <!-- Main card -->
  <rect x="100" y="150" width="600" height="200" rx="20" fill="url(#card)" stroke="#1db954" stroke-width="2"/>
  
  <!-- Spotify logo -->
  <g transform="translate(200, 200)">
    <circle cx="30" cy="30" r="25" fill="#1db954"/>
    <path d="M20 20 L40 20 L40 40 L20 40 Z" fill="#191414"/>
    <path d="M25 25 L35 30 L25 35 Z" fill="#1db954"/>
  </g>
  
  <!-- Music player interface -->
  <g transform="translate(350, 180)">
    <rect x="0" y="0" width="200" height="120" rx="10" fill="#282828" stroke="#404040" stroke-width="1"/>
    <rect x="10" y="10" width="180" height="40" rx="5" fill="#1db954"/>
    <rect x="10" y="60" width="180" height="3" rx="1.5" fill="#404040"/>
    <rect x="10" y="60" width="120" height="3" rx="1.5" fill="#1db954"/>
    <circle cx="125" cy="61.5" r="4" fill="#1db954"/>
    <rect x="10" y="80" width="60" height="15" rx="3" fill="#404040"/>
    <rect x="80" y="80" width="40" height="15" rx="3" fill="#404040"/>
    <rect x="130" y="80" width="60" height="15" rx="3" fill="#404040"/>
  </g>
  
  <!-- Text -->
  <text x="400" y="280" text-anchor="middle" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#ffffff">Spotify Clone</text>
  <text x="400" y="310" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" fill="#b3b3b3">Full-stack music streaming platform</text>
  
  <!-- Decorative elements -->
  <rect x="120" y="170" width="4" height="30" fill="#1db954" opacity="0.6"/>
  <rect x="120" y="170" width="30" height="4" fill="#1db954" opacity="0.6"/>
  <rect x="650" y="170" width="4" height="30" fill="#1ed760" opacity="0.6"/>
  <rect x="636" y="170" width="30" height="4" fill="#1ed760" opacity="0.6"/>
</svg>
